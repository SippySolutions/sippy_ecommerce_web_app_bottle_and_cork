{
  "name": "frontend",
  "private": true,
  "version": "1.3.0",
  "type": "module",
  "author": {
    "name": "SippySolutions",
    "email": "info@sippysolutions.com"
  },
  "scripts": {
    "dev": "vite --port 3003",
    "dev:https": "vite --port 3003 --https",
    "build": "vite build",
    "build:mobile": "vite build --config vite.config.mobile.js --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "cap:sync": "npx cap sync",
    "cap:sync:prod": "npx cap sync --config capacitor.config.prod.ts",
    "cap:open": "npx cap open",
    "cap:build": "npm run build:mobile && npx cap sync",
    "cap:build:prod": "npm run build:mobile && npx cap sync --config capacitor.config.prod.ts",
    "android:dev": "npm run cap:build && npx cap run android --livereload",
    "android:release": "npm run cap:build:prod && cd android && ./gradlew assembleRelease",
    "android:release:live": "npm run cap:build:prod && cd android && ./gradlew assembleRelease",
    "ios:dev": "npm run cap:build && npx cap run ios --livereload",
    "mobile:setup": "npx cap add android && npx cap add ios && npm run cap:sync"
  },
  "dependencies": {
    "@capacitor/android": "^7.3.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/cli": "^7.3.0",
    "@capacitor/core": "^7.3.0",
    "@capacitor/haptics": "^7.0.1",
    "@capacitor/ios": "^7.3.0",
    "@capacitor/keyboard": "^7.0.1",
    "@capacitor/push-notifications": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@tailwindcss/vite": "^4.1.5",
    "axios": "^1.6.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "framer-motion": "^11.0.0",
    "motion": "^12.10.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.0.0",
    "react-toastify": "^11.0.5",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.5"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "terser": "^5.42.0",
    "vite": "^6.3.5"
  }
}
