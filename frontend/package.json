{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",  "scripts": {
    "dev": "vite --port 3003",
    "dev:https": "vite --port 3003 --https",
    "dev:http": "vite --port 3003",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "setup:certs": "node -e \"console.log('Certificates are already set up! Run: npm run dev:https')\"",
    "check:certs": "node -e \"const fs=require('fs'); const exists = fs.existsSync('./certs/localhost.pem') && fs.existsSync('./certs/localhost-key.pem'); console.log(exists ? '✅ SSL certificates found' : '❌ SSL certificates not found');\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@tailwindcss/vite": "^4.1.5",
    "axios": "^1.6.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "framer-motion": "^11.0.0",
    "motion": "^12.10.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.0.0",
    "react-toastify": "^11.0.5",
    "tailwindcss": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "terser": "^5.42.0",
    "vite": "^6.3.5"
  }
}
