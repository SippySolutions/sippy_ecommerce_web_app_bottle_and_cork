{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "@vite_api_base_url",
    "VITE_MODE": "@vite_mode",
    "VITE_AUTHORIZE_NET_PUBLIC_KEY": "@vite_authorize_net_public_key",
    "VITE_AUTHORIZE_NET_API_LOGIN_ID": "@vite_authorize_net_api_login_id"
  }
}
